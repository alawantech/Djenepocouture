{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "djenepocouture.vercel.app"
        }
      ],
      "destination": "https://djenepocouture.com/",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host", 
          "value": "djenepocouture.vercel.app"
        }
      ],
      "destination": "https://djenepocouture.com/$1",
      "permanent": true
    }
  ]
}
